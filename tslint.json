{
    "extends": [
      "tslint-microsoft-contrib/recommended",
      "codelyzer",
      "rxjs-tslint-rules",
      "tslint-eslint-rules"
    ],
    "rules": {
      "adjacent-overload-signatures": true,
      "ban-types": [
        true, [
          "String"
        ],
        [
          "Array"
        ]
      ],
      "member-access": true,
      "member-ordering": [
        true,
        {
          "order": [
            "public-static-field",
            "protected-static-field",
            "private-static-field",
            "public-instance-field",
            "protected-instance-field",
            "private-instance-field",
            "public-static-method",
            "protected-static-method",
            "private-static-method",
            "public-constructor",
            "protected-constructor",
            "private-constructor",
            "public-instance-method",
            "protected-instance-method",
            "private-instance-method"
          ]
        }
      ],
      "no-any": false,
      "no-empty-interface": false,
      "no-for-in": true,
      "no-import-side-effect": false,
      "no-inferrable-types": false,
      "no-internal-module": true,
      "no-magic-numbers": false,
      "no-namespace": true,
      "no-non-null-assertion": true,
      "no-parameter-reassignment": false,
      "no-reference": true,
      "no-var-requires": true,
      "only-arrow-functions": [
        true,
        "allow-named-functions"
      ],
      "prefer-for-of": true,
      "promise-function-async": false,
      "typedef": [
        true,
        "parameter",
        "call-signature",
        "property-declaration"
      ],
      "unified-signatures": true,
      "await-promise": true,
      "ban-comma-operator": true,
      "curly": true,
      "forin": true,
      "function-constructor": true,
      "label-position": true,
      "no-arg": true,
      "no-async-without-await": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-console": [
        true,
        "log"
      ],
      "no-construct": true,
      "no-debugger": true,
      "no-duplicate-super": true,
      "no-duplicate-switch-case": true,
      "no-duplicate-variable": [
        true,
        "check-parameters"
      ],
      "no-dynamic-delete": true,
      "no-empty": false,
      "no-eval": true,
      "no-floating-promises": false,
      "no-for-in-array": true,
      "no-implicit-dependencies": [
        true,
        "dev", [
          "@libs",
          "@app",
          "@env"
        ]
      ],
      "no-inferred-empty-object-type": true,
      "no-invalid-template-strings": true,
      "no-invalid-this": true,
      "no-misused-new": true,
      "no-null-keyword": false,
      "no-null-undefined-union": true,
      "no-object-literal-type-assertion": true,
      "no-promise-as-boolean": true,
      "no-return-await": false,
      "no-shadowed-variable": true,
      "no-sparse-arrays": true,
      "no-string-literal": false,
      "no-string-throw": true,
      "no-submodule-imports": false,
      "no-switch-case-fall-through": true,
      "no-tautology-expression": true,
      "no-this-assignment": true,
      "no-unbound-method": false,
      "no-unnecessary-class": false,
      "no-unsafe-any": false,
      "no-unused-variable": true,
      "no-unused-expression": true,
      "no-var-keyword": true,
      "no-void-expression": [
        true,
        "ignore-arrow-function-shorthand"
      ],
      "prefer-conditional-expression": true,
      "prefer-object-spread": true,
      "radix": true,
      "restrict-plus-operands": true,
      "static-this": true,
      "strict-boolean-expressions": false,
      "strict-comparisons": [
        true,
        {
          "allow-object-equal-comparison": true
        }
      ],
      "strict-string-expressions": false,
      "strict-type-predicates": false,
      "switch-default": false,
      "triple-equals": [
        true,
        "allow-null-check",
        "allow-undefined-check"
      ],
      "unnecessary-constructor": false,
      "use-default-type-parameter": true,
      "use-isnan": true,
      "cyclomatic-complexity": true,
      "deprecation": false,
      "invalid-void": true,
      "max-classes-per-file": [
        true,
        2
      ],
      "max-file-line-count": false,
      "no-default-export": false,
      "no-default-import": false,
      "no-duplicate-imports": true,
      "no-require-imports": true,
      "object-literal-sort-keys": false,
      "prefer-const": true,
      "array-type": [
        true,
        "array"
      ],
      "arrow-return-shorthand": true,
      "callable-types": true,
      "class-name": true,
      "comment-format": [
        true,
        "check-space",
        {
          "ignore-words": [
            "TODO",
            "FIXME"
          ]
        }
      ],
      "comment-type": [
        true,
        "doc",
        "singleline",
        "multiline"
      ],
      "encoding": true,
      "file-name-casing": [
        true,
        "kebab-case"
      ],
      "increment-decrement": [
        true,
        "allow-post"
      ],
      "interface-name": [
        true,
        "always-prefix"
      ],
      "interface-over-type-literal": true,
      "match-default-export-name": false,
      "newline-per-chained-call": false,
      "no-angle-bracket-type-assertion": false,
      "no-boolean-literal-compare": true,
      "no-parameter-properties": false,
      "no-reference-import": true,
      "no-unnecessary-callback-wrapper": true,
      "no-unnecessary-initializer": true,
      "no-unnecessary-qualifier": true,
      "object-literal-key-quotes": [
        true,
        "as-needed"
      ],
      "object-literal-shorthand": true,
      "one-line": [
        true,
        "check-open-brace",
        "check-catch",
        "check-else",
        "check-whitespace"
      ],
      "one-variable-per-declaration": true,
      "ordered-imports": [
        true,
        {
          "import-sources-order": "any",
          "named-imports-order": "any",
          "grouped-imports": true,
          "groups": [{
            "name": "Angular imports",
            "match": "^@angular/(?!material|cdk)",
            "order": 10
          },
            {
              "name": "Angular Material imports",
              "match": "^@angular/(cdk|material)/.+",
              "order": 20
            },
  
            {
              "name": "Firestitch imports",
              "match": "^@firestitch",
              "order": 30
            },
  
            {
              "name": "RXJS imports",
              "match": "^rxjs",
              "order": 40
            },
            {
              "name": "Other package imports",
              "match": "^(?!@libs|@env|@app|\\.).+",
              "order": 50
            },
            {
              "name": "Env imports",
              "match": "^@env",
              "order": 60
            },
            {
              "name": "Libs imports",
              "match": "^@libs/.+",
              "order": 70
            },
            {
              "name": "App imports",
              "match": "^@app/.+",
              "order": 80
            },
            {
              "name": "Module imports",
              "match": "^\\.\\.",
              "order": 90
            },
            {
              "name": "Module imports",
              "match": "^\\.",
              "order": 100
            }
          ]
        }
      ],
      "prefer-function-over-method": false,
      "prefer-method-signature": false,
      "prefer-switch": [
        true,
        {
          "min-cases": 3
        }
      ],
      "prefer-template": [
        true,
        "allow-single-concat"
      ],
      "return-undefined": false,
      "space-before-function-paren": false,
      "space-within-parens": false,
      "switch-final-break": false,
      "type-literal-delimiter": true,
      "unnecessary-bind": true,
      "unnecessary-else": false,
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore"
      ],
      "align": true,
      "arrow-parens": true,
      "eofline": true,
      "import-spacing": true,
      "indent": [
        true,
        "spaces",
        2
      ],
      "max-line-length": [
        true,
        {
          "limit": 100,
          "ignore-pattern": "^import { \\w+ |^export |\\*|//|loadChildren"
        }
      ],
      "newline-before-return": true,
      "new-parens": true,
      "no-consecutive-blank-lines": [
        true,
        2
      ],
      "no-irregular-whitespace": true,
      "no-trailing-whitespace": true,
      "number-literal-format": false,
      "quotemark": [
        true,
        "single",
        "avoid-escape",
        "avoid-template"
      ],
      "semicolon": [
        true,
        "always"
      ],
      "trailing-comma": [
        true,
        {
          "esSpecCompliant": true,
          "multiline": {
            "arrays": "always",
            "functions": "always",
            "objects": "always",
            "typeLiterals": "ignore"
          }
        }
      ],
      "whitespace": [
        true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-module",
        "check-separator",
        "check-rest-spread",
        "check-type",
        "check-type-operator",
        "check-preblock"
      ],
      "chai-prefer-contains-to-index-of": true,
      "chai-vague-errors": true,
      "export-name": false,
      "function-name": [
        true,
        {
          "method-regex": "^[a-z][\\w\\d]+[$]?$",
          "private-method-regex": "^_[a-z][\\w\\d]+[$]?$",
          "protected-method-regex": "^[a-z][\\w\\d]+[$]?$",
          "static-method-regex": "^([A-Z][A-Z_\\d]*)|([a-z][\\w\\d]*)$",
          "function-regex": "^[a-z][\\w\\d]+[$]?$"
        }
      ],
      "import-name": false,
      "insecure-random": false,
      "max-func-body-length": [
        true,
        {
          "ignore-comments": true
        }
      ],
      "no-backbone-get-set-outside-model": false,
      "no-cookies": true,
      "no-delete-expression": true,
      "no-disable-auto-sanitization": true,
      "no-document-domain": true,
      "no-document-write": true,
      "no-empty-line-after-opening-brace": false,
      "no-exec-script": true,
      "no-function-expression": true,
      "no-http-string": false,
      "no-inner-html": true,
      "no-multiline-string": false,
      "no-multiple-var-decl": true,
      "no-octal-literal": true,
      "no-relative-imports": false,
      "no-single-line-block-comment": false,
      "no-string-based-set-interval": true,
      "no-string-based-set-timeout": true,
      "no-suspicious-comment": false,
      "no-typeof-undefined": true,
      "no-unnecessary-field-initialization": true,
      "no-unnecessary-local-variable": true,
      "no-unnecessary-override": true,
      "no-unnecessary-semicolons": true,
      "no-unsupported-browser-code": true,
      "no-useless-files": true,
      "no-with-statement": true,
      "prefer-array-literal": false,
      "prefer-type-cast": false,
      "promise-must-complete": true,
      "use-named-parameter": true,
      "void-zero": true,
    
      "no-constant-condition": true,
      "no-control-regex": true,
      "no-empty-character-class": true,
      "no-ex-assign": true,
      "no-extra-boolean-cast": false,
      "no-extra-semi": true,
      "no-inner-declarations": true,
      "no-invalid-regexp": true,
      "ter-no-irregular-whitespace": true,
      "no-regex-spaces": true,
      "no-unexpected-multiline": true,
      "no-multi-spaces": true,
      "ter-no-proto": true,
      "ter-no-script-url": true,
      "ter-no-self-compare": true,
      "handle-callback-err": true,
      "array-bracket-spacing": false,
      "block-spacing": [
        true,
        "always"
      ],
      "brace-style": [
        true,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "ter-computed-property-spacing": true,
      "ter-func-call-spacing": [
        true,
        "never"
      ],
      "ter-indent": [
        true,
        2,
        {
          "SwitchCase": 1,
          "VariableDeclarator": "first",
          "outerIIFEBody": 1,
          "MemberExpression": 1,
          "FunctionDeclaration": {
            "parameters": "first",
            "body": 1
          },
          "FunctionExpression": {
            "parameters": "first",
            "body": 1
          },
          "CallExpression": {
            "arguments": "first"
          }
        }
      ],
      "ter-max-len": [
        true,
        {
          "code": 100,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreImports": true,
          "ignoreComments": true,
          "ignoreRegExpLiterals": true,
          "ignoreTrailingComments": true,
          "ignoreTemplateLiterals": true,
          "ignorePattern": "loadChildren"
        }
      ],
      "ter-newline-after-var": false,
      "ter-no-mixed-spaces-and-tabs": true,
      "object-curly-spacing": [
        true,
        "always"
      ],
      "ter-padded-blocks": [
        true,
        {
          "blocks": "never",
          "classes": "always",
          "switches": "newer"
        }
      ],
      "sort-imports": false,
      "space-in-parens": [
        true,
        "never",
        {
          "exceptions": [
            "empty"
          ]
        }
      ],
      "ter-no-tabs": true,
      "ter-arrow-body-style": false,
      "ter-arrow-spacing": true,
  
      "rxjs-deep-operators": false,
      "rxjs-just": false,
      "rxjs-no-async-subscribe": true,
      "rxjs-finnish": {
        "options": [{
          "methods": false,
          "variables": true,
          "functions": false,
          "parameters": false,
          "properties": true,
          "names": {
            "Stream$": false
          },
          "types": {
            "^EventEmitter$": false
          }
        }],
        "severity": "error"
      },
      "rxjs-no-connectable": true,
      "rxjs-no-create": true,
      "rxjs-no-do": false,
      "rxjs-no-explicit-generics": false,
      "rxjs-no-exposed-subjects": true,
      "rxjs-no-index": true,
      "rxjs-no-internal": true,
      "rxjs-no-nested-subscribe": true,
      "rxjs-no-redundant-notify": true,
      "rxjs-no-subclass": true,
      "rxjs-no-subject-unsubscribe": true,
      "rxjs-no-subject-value": false,
      "rxjs-no-unbound-methods": true,
      "rxjs-no-unsafe-catch": true,
      "rxjs-no-unsafe-first": true,
      "rxjs-no-unsafe-scope": false,
      "rxjs-no-unsafe-switchmap": true,
      "rxjs-no-unsafe-takeuntil": true,
      "rxjs-prefer-angular-async-pipe": false,
      "rxjs-prefer-angular-composition": false,
      "rxjs-prefer-angular-takeuntil": true,
      "rxjs-prefer-observer": true,
      "rxjs-suffix-subjects": false,
  
      "contextual-decorator": true,
      "contextual-lifecycle": true,
      "no-attribute-decorator": false,
      "no-lifecycle-call": true,
      "no-output-native": true,
      "no-pipe-impure": true,
      "prefer-on-push-component-change-detection": false,
      "template-accessibility-elements-content": true,
      "template-accessibility-tabindex-no-positive": true,
      "template-accessibility-table-scope": true,
      "template-banana-in-box": true,
      "template-no-any": true,
      "template-no-autofocus": true,
      "template-no-negated-async": true,
      "use-lifecycle-interface": true,
      "component-max-inline-declarations": true,
      "no-conflicting-lifecycle": true,
      "no-forward-ref": false,
      "no-input-prefix": true,
      "no-input-rename": false,
      "no-output-on-prefix": true,
      "no-output-rename": false,
      "no-unused-css": true,
      "relative-url-prefix": true,
      "template-conditional-complexity": true,
      "template-cyclomatic-complexity": false,
      "template-no-call-expression": false,
      "template-use-track-by-function": false,
      "use-component-selector": false,
      "use-component-view-encapsulation": false,
      "use-pipe-decorator": true,
      "use-pipe-transform-interface": true,
      "component-class-suffix": [
        true,
        "Component",
        "Container",
        "View"
      ],
      "directive-class-suffix": true,
      "import-destructuring-spacing": true,
      "no-host-metadata-property": false,
      "no-inputs-metadata-property": false,
      "no-outputs-metadata-property": false,
      "no-queries-metadata-property": true,
      "prefer-inline-decorator": false
    }
  }
  